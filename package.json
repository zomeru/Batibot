{
  "name": "batibot",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "dev": "concurrently \"pnpm run watch:compile\" \"pnpm run watch:dev\"",
    "watch:compile": "swc src -w --out-dir dist",
    "watch:dev": "nodemon --watch \"dist/**/*\" -e js ./dist/index.js",
    "build": "rimraf dist && swc src -d dist",
    "start": "node dist/index.js"
  },
  "author": "Zomer Gregorio",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "node": "^20.2.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.60",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "chokidar": "^3.5.3",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
